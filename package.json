  "scripts": {
    "rimraf": "rimraf packages/ && rimraf client_packages/client && rimraf client_packages/html",
    "build": "concurrently \"npm run build:client\" \"npm run build:server\" \"npm run build:web\"",
    "watch": "concurrently \"npm run watch:client\" \"npm run watch:server\" \"npm run watch:web\"",
    "watch:web": "cross-env NODE_ENV=development webpack --watch --progress",
    "web:dev": "webpack-dev-server --mode development --open --hot",
    "watch:server": "parcel watch src/server/index.ts --out-dir packages/server --target node",
    "watch:client": "parcel watch src/client/index.ts --out-dir client_packages/client --target node && node varInit.js",
    "storybook": "start-storybook",
    "build:web": "cross-env NODE_ENV=development webpack --progress",
    "build:server": "parcel build src/server/index.ts --no-minify --out-dir packages/server --target node",
    "build:client": "parcel build src/client/index.ts --no-minify --out-dir client_packages/client --target node && node varInit.js",
    "production": "rimraf client_packages/client && concurrently \"parcel build src/client/index.ts --out-dir client_packages/client --target node --no-source-maps && node varInit.js\" \"parcel build src/server/index.ts --no-minify --out-dir packages/server --target node\" \"cross-env NODE_ENV=development webpack --progress\"",
